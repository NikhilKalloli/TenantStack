# API Service
TENANTSTACK_API_PORT=3000
TENANTSTACK_API_VERSION=v1
MONGODB_URI=mongodb://tenantstack-mongodb:27017/tenantstack

# Tenant Manager
TENANTSTACK_TENANT_MANAGER_PORT=3001
TENANTSTACK_CONTENTSTACK_API_KEY=your_contentstack_api_key
TENANTSTACK_CONTENTSTACK_ACCESS_TOKEN=your_contentstack_access_token

# Auth Service
TENANTSTACK_AUTH_PORT=3002
TENANTSTACK_AUTH_SECRET=your_jwt_secret_key
TENANTSTACK_AUTH_EXPIRES=24h

# Audit Logger
TENANTSTACK_AUDIT_PORT=3003
TENANTSTACK_AUDIT_RETENTION=90

# Redis
REDIS_URL=redis://tenantstack-redis:6379
